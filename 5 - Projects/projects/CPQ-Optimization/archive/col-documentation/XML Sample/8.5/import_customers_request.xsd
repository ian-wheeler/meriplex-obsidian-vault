<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" elementFormDefault="qualified" xml:lang="EN">
	<xs:include schemaLocation="coTypes.xsd"/>
	<xs:element name="import_customers_request">
		<xs:complexType>
			<xs:complexContent>
				<xs:extension base="COEnvelope">
					<xs:sequence>
						<xs:element name="customers">
							<xs:complexType>
								<xs:sequence>
									<xs:element name="customer" type="COCustomerDetail" minOccurs="0" maxOccurs="unbounded"/>
								</xs:sequence>
								<xs:attribute name="useBillingCodeIdentifier" type="xs:boolean"/>
								<xs:attribute name="useShippingCodeIdentifier" type="xs:boolean"/>
							</xs:complexType>
						</xs:element>
					</xs:sequence>
				</xs:extension>
			</xs:complexContent>
		</xs:complexType>
	</xs:element>
</xs:schema>
